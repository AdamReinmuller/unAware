    "apollo-cache-inmemory": "1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "1.5.16",
    "apollo-link-token-refresh": "^0.2.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^1.10.7",
    "date-fns": "^2.11.1",
    "downshift": "^5.0.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
