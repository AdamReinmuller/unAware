{
  "name": "unaware-frontend",
  "version": "1.0.0",
  "description": "unAware! An example React, GraphQL, Next and Apollo",
  "main": "index.js",
  "scripts": {
    "start": "next -p 7777",
    "developer": "NODE_OPTIONS='--inspect' next",
